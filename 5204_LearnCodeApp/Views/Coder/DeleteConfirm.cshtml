@model _5204_LearnCodeApp.Models.CoderDto

@{
    ViewBag.Title = "DeleteConfirm";
}




    <h2>Delete Confirmation</h2>

    <h3>Are you sure you want to delete this profile?</h3>
    <div>

        <dl class="dl-horizontal">

            <dt>
                @Html.DisplayNameFor(model => model.FirstName)
            </dt>
            <dd>
                @Html.DisplayFor(model => model.LastName)
            </dd>

            <dt>
                @Html.DisplayNameFor(model => model.UserName)
            </dt>
            <dt>
                @Html.DisplayNameFor(model => model.CoderEmail)
            </dt>
            <dt>
                @Html.DisplayNameFor(model => model.CoderBio)
            </dt>



        </dl>

        @using (Html.BeginForm("Delete", "Coder", new { id = Model.CoderID }))
        {
            @Html.AntiForgeryToken()

            <div class="form-actions no-color">
                <input type="submit" value="Delete" class="btn btn-default" /> |
                @Html.ActionLink("Back to List", "List")
            </div>
        }
    </div>
 

